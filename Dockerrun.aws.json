{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
        "name": "client",
        "image": "cheonu/multi-account",
        "hostname": "client",
        "essential": false
        },
        {
        "name": "server",
        "image": "cheonu/multi-server",
        "hostname": "api",
        "essential": false
        },
        {
        "name": "worker",
        "image": "cheonu/multi-worker",
        "hostname": "worker",
        "essential": false
        },
        {
        "name": "nginx",
        "image": "cheonu/multi-nginx",
        "essential": true,
        "portMappings": [
            {
                "hostport": 80,
                "containerPort": 80
            }
        ],
        "links": ["client", "server"]
        }
    ]
}